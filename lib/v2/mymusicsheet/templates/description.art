<div class="item-description">
    {{if videoId}}
    <iframe src="https://www.youtube.com/embed/{{videoId}}?autoplay=0" style="width:100%; height:100%;" frameborder="0" allow="accelerometer; autoplay; clipboard-write; encrypted-media; gyroscope; picture-in-picture" allowfullscreen></iframe>
    {{/if}}

    {{if content}}
        {{each content}}
        <p>{{$value}}</p>
        {{/each}}
    {{/if}}
</div>

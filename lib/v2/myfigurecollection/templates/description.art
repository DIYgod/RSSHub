{{ if pictures }}
{{ each pictures picture }}
<img src="{{ picture.src }}" weight="{{ picture.w }}" height="{{ picture.h }}">
{{ /each }}
{{ /if }}
{{ if fields }}
<table>
<tbody>
{{ each fields field }}
<tr>
<th>{{ field.key }}</th>
<td>{{ field.value }}</td>
</tr>
{{ /each }}
</tbody>
</table>
{{ /if }}
<img src="{{ mod.pic }}" referrerpolicy="no-referrer">
{{ each mod.label l }}
  <p>{{ l }}</p>
{{ /each }}
{{ if mod.support.length > 0 }}
  <p>支持的MC版本: </p>
  <ul>
    {{ each mod.support s }}
      <li><b>{{ s.label }}</b>{{ s.versions }}</li>
    {{ /each }}
  </ul>
{{ /if }}
<hr style="height: 2px; background-color: #e7e7e7; border: 0 none;">
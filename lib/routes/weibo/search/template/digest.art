<div class="quoted">
    <a style="text-decoration: none;" href="{{ author.link }}">{{ author.name }}</a>
    <span><a href="{{link }}"> | {{ postinfo }} </a></span>
</div>
<div class="content">
{{@ msg }}
</div>

{{if picnum > 0 }}
    <br>
    <div class='pic-row'>
    {{each pics}}
        <a href="{{$value.rurl}}"><img src="{{$value.url}}" ></a>
    {{/each}}
    </div>
{{/if}}
<hr>

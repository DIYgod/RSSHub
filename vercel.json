{
  "version": 2,
  "builds": [
    {
      "src": "api/index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "api/index.js"
    }
  ],
  "installCommand": "pnpm install --no-frozen-lockfile",
  "functions": {
    "api/index.js": {
      "runtime": "nodejs22.x"
    }
  }
}


